<%= form_with url:"/directors/#{@director.id}/films", method: :post, local: true do |form|  %>
  <%= form.label :name %></br>
  <%= form.text_field :name %></br>
  <%= form.label :runtime %></br>
  <%= form.text_field :runtime %></br>
  <%= form.label :streaming_on_netflix  %></br>
  <%= form.text_field :streaming_on_netflix %></br>
  </br>
  <%= form.submit "Add Film" %>
<% end %>